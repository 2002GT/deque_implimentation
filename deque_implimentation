#include <iostream>
#include<deque> 
using namespace std;

int main()
{
    // PUSH----------
    
    deque<int>dq;
    dq.push_back(1);
    dq.push_back(2);
    dq.push_front(3);
    dq.push_front(4);
    
    for(auto i : dq)
    cout<<i<<" ";
    cout<<endl;
    
    // POP----------------------------
    
    dq.pop_back();
    dq.pop_front();
    
    for(auto i : dq)
    cout<<i<<" ";
    cout<<endl;
    
    // SIZE----------------
    
    
    cout<<dq.size();

    return 0;
}
